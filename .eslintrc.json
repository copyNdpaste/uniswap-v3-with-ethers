{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "quotes": ["error", "single"],
        "semi": ["error", "always"],
        "indent": ["error", 2]
    },
    "overrides": [{
        "files": ["test-*.js"],
        "rules": {
            "rule-name": "off"
        }
    }]
    
}
